
<% layout("/layouts/boilerplate") %>
    
<body>
    <h1>Quick Actions</h1>
    <div class="quick-actions">
        <a href="#upload-section">Upload Resume</a>
        <a href="https://www.careerservices.com">Career Advice</a>
        <a href="#chat-section">Chat with Us</a>
    </div>

    <!-- Resume Upload -->
    <div id="upload-section">
        <h3>Upload Your Resume</h3>
        <form action="/upload" method="POST" enctype="multipart/form-data">
            <input type="file" name="resumeFile" accept=".pdf, .doc, .docx" required>
            <button type="submit">Upload Resume</button>
        </form>
    </div>

    <!-- Auto Generate Resume -->

   
    
</body>
